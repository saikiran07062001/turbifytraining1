<?xml version='1.0' encoding='UTF-8'?>
<!DOCTYPE hibernate-mapping SYSTEM
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="org.infinite.pojo.Login" table="Login3">
	<id name="loginid" type="int" column="loginid">
			<generator class="native"></generator>
		</id>
		<property name="username" column="User_name" type="string"></property>
		<property name="password" column="password" type="string"></property>
		<property name="time" column="time" type="timestamp"></property>
	</class>
</hibernate-mapping>